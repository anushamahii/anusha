<?xml version="1.0" encoding="UTF-8"?>
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
  
<bean id="adr" class="bhanu.address">  
<property name="strt" value="jnrstreet"></property>  
<property name="city" value="mpl"></property>
<property name="state" value="andharapradesh"></property>
 

</bean>  
  
  
<bean id="customer" class="bhanu.customer">  
<property name="id" value="234"></property>  
<property name="Name" value="kavya"></property>
<property name="Email" value="gadamanianusha@gmail.com"></property>
<property name="adr" ref="Adr"></property>  

</bean>  
</beans> 